<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd"
                   logicalFilePath="ara/00000450_create_table_cycle_definition.xml">

    <changeSet id="00000450_create_table_cycle_definition" author="slaout">
        <createTable tableName="cycle_definition">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="name" type="varchar(16)">
                <constraints nullable="false"/>
            </column>
            <column name="branch" type="varchar(16)">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <modifySql dbms="mysql">
            <append value=" ENGINE=INNODB ROW_FORMAT=DYNAMIC CHARSET=UTF8 COLLATE utf8_unicode_ci"/>
        </modifySql>
    </changeSet>

</databaseChangeLog>
