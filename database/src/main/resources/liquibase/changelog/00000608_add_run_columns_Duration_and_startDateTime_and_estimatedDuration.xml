<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd"
                   logicalFilePath="ara/00000608_add_run_columns_Duration_and_startDateTime_and_estimatedDuration.xml">

    <changeSet id="00000608_add_run_columns_Duration_and_startDateTime_and_estimatedDuration" author="slaout">
        <addColumn tableName="run">
            <column name="start_date_time" type="datetime">
                <constraints nullable="true"/>
            </column>
            <column name="estimated_duration" type="bigint">
                <constraints nullable="true"/>
            </column>
            <column name="duration" type="bigint">
                <constraints nullable="true"/>
            </column>
        </addColumn>
    </changeSet>
    
</databaseChangeLog>








