<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd"
                   logicalFilePath="ara/00001203_add_scenario_wrong_country_code_column_and_wrong_severity_code_column.xml">

    <changeSet id="00001203_add_scenario_wrong_country_code_column_and_wrong_severity_code_column" author="slaout">
        <addColumn tableName="scenario">
            <column name="wrong_country_codes" type="varchar(128)">
