    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "test/cypress/support/stepDefinitions",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "test/cypress/outputs/cucumber",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
