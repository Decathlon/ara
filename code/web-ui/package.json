{
  "name": "ara-web-ui",
  "version": "9.6.0",
  "description": "Agile Regression Analyzer front app",
  "author": "Decathlon <developers@decathlon.com>",
  "private": true,
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "build": "node build/build.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "cy:all": "cypress run --spec 'test/cypress/**/*.feature'",
    "cy:wip": "cypress run -e TAGS='@wip' -e GLOB=test/cypress/**/*.feature",
    "cy:output:cleanup": "rm -fr test/cypress/outputs",
    "cy:report:cleanup": "rm -fr test/cypress/outputs/reports",
    "cy:report:generate": "npm run cy:report:cleanup; cypress run",
    "cy:report:merge": "npm run cy:report:generate; mkdir -p test/cypress/outputs/reports/json/merged && npx mochawesome-merge 'test/cypress/outputs/reports/json/all/*.json' > test/cypress/outputs/reports/json/merged/final-report.json",
    "cy:report:html": "npm run cy:report:merge; npx marge test/cypress/outputs/reports/json/merged/final-report.json --overwrite=true --charts=true --reportDir=test/cypress/outputs/reports/html"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^6.0.1",
    "downloadjs": "^1.4.7",
    "eslint-plugin-html": "^3.1.1",
    "html-to-image": "^1.6.2",
    "iview": "^3.1.3",
    "moment": "^2.22.2",
    "sharp": "^0.26.3",
    "vue": "2.5.17",
    "vue-cookies": "^1.7.4",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.1",
    "vue-virtual-scroller": "^0.12.2",
    "vue2-filters": "^0.3.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
